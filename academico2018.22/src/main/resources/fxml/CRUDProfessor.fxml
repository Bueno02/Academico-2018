<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.materialdesignicons.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CRUDProfessorController">

   <children>
      <VBox alignment="BOTTOM_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="433.0">
               <children>
                  <VBox prefHeight="47.0" prefWidth="139.0" spacing="5.0">
                     <children>
                        <Label text="CPF" />
                        <TextField fx:id="txtFldCpf" prefHeight="25.0" prefWidth="111.0" />
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox prefHeight="47.0" prefWidth="160.0" spacing="5.0">
                     <children>
                         <Label text="Nome" />
                        <TextField fx:id="txtFldNome" maxWidth="200.0" prefWidth="245.0" />
                     </children>
                     <HBox.margin>
                        <Insets left="10.0" right="5.0" />
                     </HBox.margin>
                  </VBox>
                    
                  <VBox prefHeight="47.0" prefWidth="129.0" spacing="5.0">
                     <children>
                        <HBox prefHeight="0.0" prefWidth="81.0" spacing="5.0">
                           <children>
                               <Label text="Cidade" />
                               <MaterialDesignIconView fx:id="btnIncluir" glyphName="PLUS" onMouseClicked="#acIncluir" size="1.3em" />
                               <MaterialDesignIconView fx:id="btnExcluir" glyphName="MINUS" onMouseClicked="#acExcluir" size="1.3em" />
                           </children>
                        </HBox>
                        <ComboBox fx:id="cmbCidade" prefHeight="25.0" prefWidth="132.0" />
                        
                     </children>
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" spacing="5.0">
               <children>
                  <VBox>
                     <children>
                         <Label text="Email" />
                        <TextField fx:id="txtFldEmail" prefHeight="25.0" prefWidth="230.0" />
                        
                     </children>
                  </VBox>
                  <Button fx:id="btnConfirma" mnemonicParsing="false" onAction="#btnConfirmaClick" prefWidth="100.0" text="Confirma">
                     <graphic>
                        <MaterialDesignIconView glyphName="CHECK" />
                     </graphic>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#btnCancelaClick" prefWidth="100.0" text="Cancela">
                     <graphic>
                        <MaterialDesignIconView glyphName="CLOSE" />
                     </graphic>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
